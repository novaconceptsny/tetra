	/>	

	<start tx="-800" ty="-165" tz="-500"  hlookat="90" vlookat="0" oz="3000"/>
	<view  tx="-800" ty="-165" tz="-500"  hlookat="90" />

	<scene name="dollhouse" autoload="true" title="hello" >
		<image>
			<depthmap backgroundurl="skybox.jpg" url="models/gallery_130_2.obj" subdiv="0" rendermode="3dmodel" hittest="true" scale="125" textured="true" waitforload="true" center="0,0,0" axis="+x+y+z"/> 
		</image>


		<!-- <layer name="credits" visible="true" type="container" align="topright" flowchildren="v">
			<layer name="author" bgalpha="0.5" alpha="0.5" align="topright" type="text" text="Model by kulturschokolade.org" onclick="openurl('https://sketchfab.com/3d-models/gallery-6041625d66084a1fb593316c949a2ed3')"></layer>
