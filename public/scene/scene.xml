		</layer> -->
		
		<layer align="center" name="bgimagecontainer" type="container" width="100%" height="100%" bgcolor="0xff0000" bgblur="20" visible="true" >
			<layer align="center" flowchildren="v" name="centering" type="container" width="100%" height="100" bgcolor="0xff0000"  visible="true" >
				<layer  name="loading" css="color: white; font-size: 2vh"  type="text"  align="bottom" text-align="center" text="Loading...[BR /][PROGRESS style='width: 200px; accent-color: #dd0000;' /]" textcolor="0xffffff" bgalpha="0"></layer>
			</layer>
		</layer>
		
		<events onloadcomplete="tween(layer[bgimagecontainer].bgblur, 0,1); tween(layer[loading].alpha, 0,1); delayedcall(1, set(layer[bgimagecontainer].visible, false);set(layer[loading].visible, false));"></events>
	</scene>
	
	
</krpano>