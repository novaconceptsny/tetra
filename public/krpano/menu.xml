<!-- Guideline -->

<layer name="guideline"
       url="/assets/img/user_instruction.png"
       keep="true"
       visible="false"
       enabled="true"
       handcursor="false"
       maskchildren="false"
       scalechildren="true"
       capture="true"
       zorder="60"
       align="center"
       edge="center"
       alpha="1.0"
       onloaded="d_resize();"
       scale="1"
/>

<layer name="guideline_back"
       type="container"
       keep="true"
       width="100%"
       height="100%"
       bgalpha="0.8"
       zorder="15"
       bgcolor="0x000000"
       bgcapture="true"
       visible="false"
       handcursor="false"
       align="center"
       onclick="set(visible,false); set(layer[guideline].visible,false);"
/>
</krpano>
