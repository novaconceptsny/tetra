<!-- name button -->
<layer name="name"
       type="image"
       url="/assets/img/tetra-logo.png"
       keep="true"
       parent=""
       visible="true"
       enabled="true"
       handcursor="ture"
       zorder="10"
       align="topleft"
       edge="topleft"
       height="110"
       width="prop"
       x="30"
       y="30"
       onclick=""
       alpha="1.0"
/>

<!-- NOVA button -->
<layer name="nova_dev_name"
       type="image"
       url="/assets/img/nova_dev1.png"
       keep="true"
       parent=""
       visible="true"
       enabled="true"
       handcursor="false"
       zorder="10"
       align="bottomleft"
       edge="bottomleft"
       x="30"
       y="15"
       onclick="openurl('', _self);"
       scale="0.2"
       alpha="0.8"
/>


<!-- versions button -->
<layer name="version"
       type="image"
       url="/assets/img/version_new.png"
       keep="true"
       parent=""
       visible="true"
       enabled="true"
       handcursor="true"
       zorder="11"
       align="topleft"
       edge="topleft"
       x="120"
       y="140"
       onover="set(layer[version].url,'/assets/img/version_invert.png');"
       onout="set(layer[version].url,'/assets/img/version_new.png');"
       scale="1"
       alpha="1.0"
/>


<!-- guide button -->
<layer name="guide"
       type="image"
       url="/assets/img/guide_new.png"
       keep="true"
       parent=""
       visible="true"
       enabled="true"
       handcursor="true"
       capture="false"
       zorder="11"
       align="topleft"
       edge="topleft"
       x="210"
       y="140"
       alpha="1.0"
       scale="1"
       onclick="cleanSel();switch(layer[guideline].visible); switch(layer[guideline_back].visible);hideIndicator();"
       onover="set(layer[guide].url,'/assets/img/guide_invert.png');"
       onout="set(layer[guide].url,'/assets/img/guide_new.png');"
/>


<!-- logout button -->
<layer name="logout"
       type="image"
       url="/assets/img/logout.png"
       keep="true"
       parent=""
       visible="true"
       enabled="true"
       handcursor="true"
       capture="false"
       zorder="11"
       align="topleft"
       edge="topleft"
       x="300"
       y="140"
       alpha="1.0"
       scale="1"
       onclick="openurl('/logout');"
       onover="set(layer[logout].url,'/assets/img/logout_invert.png');"
       onout="set(layer[logout].url,'/assets/img/logout.png');"
/>


