{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        },
        {
            "type": "composer",
            "url": "https://wire-elements-pro.composer.sh"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "itsgoingd/clockwork": "^5.1",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.2.0",
        "livewire/livewire": "^3.1.0",
        "opcodesio/log-viewer": "^3.1",
        "pusher/pusher-php-server": "^7.0",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-medialibrary-pro": "^3.0.0",
        "spatie/laravel-permission": "^6.0",
        "spatie/laravel-schemaless-attributes": "^2.3",
        "staudenmeir/belongs-to-through": "^2.12",
        "staudenmeir/eloquent-has-many-deep": "^1.15",
        "vinkla/hashids": "^11.0",
        "wire-elements/pro": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.10",
        "phpunit/phpunit": "^10.4",
        "spatie/laravel-ignition": "^2.3"
    },
    "autoload": {
        "files": [
            "app/Helpers/general.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
